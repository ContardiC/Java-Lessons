 package spacecoding.esercitazioni.libri;
 
 public class BooksManager {
+
+    public static Book[] listByAuthor(Book[] books, String authorFirstName, String authorLastName ){
+        Book[] bookByAuthor=new Book[100];
+
+        int j;
+        j=0;
+        for(int i=0;i<books.length;i++){
+            if(books[i].getAuthorFirstName().equals(authorFirstName)&& books[i].getAuthorLastName().equals(authorLastName)){
+                bookByAuthor[j]=books[i];
+                j++;
+            }
+        }
+        return bookByAuthor;
+    }
+
+    public static void main(String[] args) {
+        Book[] libri=new Book[100];
+        Book[] bookByAuthor=new Book[100];
+        bookByAuthor=BooksManager.listByAuthor(libri,"Salvatore Quasimodio");
+    }
+
 }
